        <jsScript_script>// Arrays para os nomes em Português (PT-BR)
var meses = ["Janeiro", "Fevereiro", "Março", "Abril", "Maio", "Junho", "Julho", "Agosto", "Setembro", "Outubro", "Novembro", "Dezembro"];
var dias_semana = ["Domingo", "Segunda-feira", "Terça-feira", "Quarta-feira", "Quinta-feira", "Sexta-feira", "Sábado"];

// --- CORREÇÃO v2 ---
// O 'Data_Completa' já é um objeto Date do Java. Não precisamos do 'new Date()'.
// Vamos usá-lo diretamente, mas com os métodos corretos do Java.
var d = Data_Completa; 

// --- Extrai os valores ---
// 1. Valores Numéricos
// A principal mudança está aqui:
// O objeto Date do Java usa .getYear() que retorna (ano - 1900).
var Ano = d.getYear() + 1900; 
var Mes = d.getMonth() + 1; // .getMonth() no Java também é 0-11
var Dia = d.getDate(); // .getDate() funciona igual

// 2. Valores de Texto (em PT-BR)
var Nome_Mes = meses[d.getMonth()];
var Dia_da_Semana = dias_semana[d.getDay()]; // d.getDay() no Java também é 0 (Dom) - 6 (Sab)

// 3. Trimestre
var Trimestre_Num = Math.floor(d.getMonth() / 3) + 1;
var Trimestre = "T" + Trimestre_Num;

// 4. Flags (Booleanos)
var Flag_Fim_de_Semana = (d.getDay() == 0 || d.getDay() == 6); // 0=Domingo, 6=Sábado

// 5. Semana do Ano (NOVO)
// Para obter a semana do ano de forma correta,
// podemos usar o java.util.Calendar, já que 'd' é um objeto Date do Java.
// Isso garante que estamos acessando as classes nativas do Java.
var cal = java.util.Calendar.getInstance();
cal.setTime(d);
var Semana_do_Ano = cal.get(java.util.Calendar.WEEK_OF_YEAR);


// Certifique-se de que os novos campos (Ano, Mes, Dia, Nome_Mes, etc.)
// estão definidos na aba "Fields" na parte inferior do step
// para que eles sejam adicionados ao fluxo.

</jsScript_script>
